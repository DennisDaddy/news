<% provide(:title, "Edit Category") %>

    <h4 align="center">Edit Category</h4>
   
    <hr>
   <%= form_for @category do |f| %>
   

      <%= f.label :name %>
      <%= f.text_field :name, class: 'form-control' %>

      <%= f.submit "Update Category", class: "btn btn-success" %>
    <% end %>
		 
                   